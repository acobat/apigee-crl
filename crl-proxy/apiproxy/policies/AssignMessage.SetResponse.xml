<AssignMessage name="AssignMessage.SetResponse">
  <Set>
    <Headers>
      <Header name="Content-Type">application/json;charset=UTF-8</Header>
    </Headers>
    <Payload contentType="application/json">
{
  "crlDistributionPoint": "{custom.crlDistributionPoint}",
  "numberOfRevokedCertificates": {custom.numberOfRevokedCertificates},
  "serialNumber": "{custom.serialNumber}",
  "crlSource": "{custom.internal.crlSource}",
  "isCertificateRevoked": {custom.isCertificateRevoked}
}
    </Payload>
  </Set>
</AssignMessage>

